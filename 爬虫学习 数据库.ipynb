   "source": [
    "import pymysql\n",
    "db = pymysql.connect()\n",
    "cursor = db.cursor()\n",
    "cursor.execute('SELECT VERSION()')\n",
    "data = cursor.fetchone()\n",
    "print('Database version:' data)\n",
    "cursor.execute('CREATE DATABASE spiders DEFAULT CHARACTER SET utf8')\n",
    "db.close()\n"
